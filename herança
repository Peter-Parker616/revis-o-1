class Animal:
    def fazer_som(self):
        pass  # Método que pode ser sobrescrito pelas subclasses

class Cachorro(Animal):
    def fazer_som(self):
        return "Au au!"

class Gato(Animal):
    def fazer_som(self):
        return "Miau!"

# Criando objetos das classes derivadas
cachorro = Cachorro()
gato = Gato()

print(cachorro.fazer_som())  # Saída: Au au!
print(gato.fazer_som())  # Saída: Miau!
